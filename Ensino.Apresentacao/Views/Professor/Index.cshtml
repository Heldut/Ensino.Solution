@{
    ViewData["Title"] = "Professores";
}
<!-- Main content -->
    <section class="content">

        <div class="row">

            <div class="col-12">

                <div class="card">
                    <div class="card-header">
                        <h3 class="card-title">Professores</h3>
                    </div>
                    <div class="card-body">

                        <table id="tabelaProfessor"
                               class="table table-bordered table-striped"
                               style="width:100%">
                            <thead>
                                <tr>
                                    <th>ID</th>
                                    <th>Nome</th>
                                    <th>Alunos</th>
                                </tr>
                            </thead>
                        </table>

                    </div>

                </div>

            </div>

        </div>

    </section>
@section scripts{
    <script src="~/js/ProfessorList.js"></script>
}